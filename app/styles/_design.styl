$palette_all=('red' $red $white) ('pink' $pink $white) ('purple' $purple $white) ('deep-purple' $deep-purple $white) ('indigo' $indigo $white) ('blue' $blue $white) ('light-blue' $light-blue $white) ('cyan' $cyan $white) ('teal' $teal $white) ('green' $green $white) ('light-green' $light-green $white) ('lime' $lime $black) ('yellow' $yellow $black) ('amber' $amber $black) ('orange' $orange $black) ('deep-orange' $deep-orange $white) ('brown' $brown $white) ('grey' $grey $white) ('blue-grey' $blue-grey $white)

.menu-open header,
.page-scroll header
  box-shadow 0 1px 10px rgba(0, 0, 0, 0.5)

header,
header a
  color #fff

.navbar-back.menu-open
  position fixed
  top 0
  left 0
  height 100%
  width 100%
  background-color #000000
  opacity 0.5
  filter unquote("alpha(opacity=50);")
  z-index 10

.navbar.navbar-fixed-top
  height 60px
  min-height 60px
  border-width 0

.navbar .nav a
  outline none
  display block
  padding 12px 16px

.navbar .nav, .navbar .nav .a, .navbar .nav a, .navbar .nav li
  position relative

.menu-toggle:focus,
.menu-toggle:hover,
.navbar .nav a:focus,
.navbar .nav a:hover
  background-color transparent !important
  text-decoration none

.navbar-logo
  display block
  float left
  padding 10px 5px
  line-height 28px
  font-size 20px
  margin-left 20px

.navbar-logo:focus,
.navbar-logo:hover
  text-decoration none

.navbar .nav-list li
  float left

.navbar .nav > li > a
  line-height 32px

.navbar .nav > li > a .navbar-bars,
.navbar .nav > li > a .navbar-close
  left 10px
  position absolute
  text-align center
  top 10px

.navbar .nav > li > a .navbar-close
  opacity 0
  filter unquote("alpha(opacity=0)")
  transform rotate(-225deg)

.navbar .nav > li > a .avatar,
.navbar .nav > li > a .icon
  transition-delay 0s
  transition-duration 0.3s
  transition-property opacity, transform
  transition-timing-function cubic-bezier(0.4, 0, 0.2, 1)

.navbar .nav > li.active > a .avatar,
.navbar .nav > li.active > a .icon
  opacity 0
  filter unquote("alpha(opacity=0)")
  transform rotate(225deg)

.navbar .nav > li.active > a .navbar-close
  opacity 1
  filter unquote("alpha(opacity=100)")
  transform rotate(0deg)

.navbar .nav > li > a .navbar-bars i,
.navbar .nav > li > a .navbar-close i
  height 36px
  width 36px
  line-height 33px

.navbar .nav > li > a .navbar-close i
  color black
  background-color #f5f5f5
  border-radius 50%
  transform scale(0)
  transition transform 0.3s cubic-bezier(0.4, 0, 0.2, 1) 0s

.navbar .nav > li.active > a .navbar-close i
  transform scale(1)

.nav.nav-list.pull-right
  margin-right 5px

.menu
  bottom 0
  left -100%
  position fixed
  top 48px
  transition-property box-shadow, transform
  width 100%
  z-index 29
  border-right 1px solid #777777

.menu, .menu.open
  transition all 0.3s cubic-bezier(0.4, 0, 0.2, 1) 0s

.menu.open
  box-shadow 0 0 10px rgba(0, 0, 0, 0.5)
  left 0

@media only screen and (min-width: 480px)
  .menu
    left -320px
    width 320px

.menu.menu-right
  left auto
  right -100%
  border-right none
  border-left 1px solid #777777

.menu.menu-right.open
  right 0

@media only screen and (min-width: 480px)
  .menu.menu-right
    right -320px

  .menu-open.right .fbtn-container
    right 336px

  .menu-open.right .scroll-to-top
    right 330px

.menu-backdrop
  display none

.menu-top-img
  align-items center
  height 100%
  justify-content center
  left 0
  position absolute
  top 0
  width 100%

.menu-top-img img
  min-height 100%
  opacity 0.5
  filter unquote("alpha(opacity=50)")
  width 100%

.menu-top-info
  padding 36px 16px
  position relative

.avatar
  background-color #e0e0e0
  border-radius 50%
  color #212121
  display block
  height 48px
  line-height 48px
  width 48px
  text-align center
.avatar > *
  line-height 48px

.navbar .avatar
  height 36px
  line-height 36px
  width 36px
.navbar .avatar > *
  line-height 36px

.avatar.pull-left
  margin-right 16px

.fbtn-container, .navbar, .menu
  backface-visibility: hidden;

.menu-content
  text-align left

.menu-content
  padding-bottom 12px
  padding-top 12px

.menu-content
  padding-bottom 12px
  padding-top 12px

.menu-content hr
  border-top-color #e0e0e0
  margin-top 11px

.menu-content .nav
  margin-bottom 0
  margin-top 0

.menu-content .nav a
  line-height 30px
  color #616161
  font-weight 700
  overflow('ellipsis')

.menu-content .nav ul a
  font-weight 400
  padding-left 32px

.menu-content .nav ul ul a
  font-size 90%
  font-weight 300
  padding-bottom 6px
  padding-left 48px
  padding-top 6px

@media only screen and (min-width: 480px)
  .menu-content
    width 320px

.menu-content-inner
  padding-left 16px
  padding-right 16px

.menu-scroll,
.menu-top-img
  overflow hidden

.menu-scroll
  background-color #fff
  height 100%
  left 0
  position absolute
  top 0
  width 100%
  z-index 1

.menu-top
  background-color: #212121
  color #f5f5f5
  position relative
  z-index 1

.menu-top
  height 125px

.menu-top a
  color #f5f5f5
  text-decoration none

.menu-top a:focus,
.menu-top a:hover
  color #fff

@media only screen and (min-width: 480px)
  .menu-top
    width 320px

.menu-top-form
  margin-bottom 6px
  margin-top 6px

.menu-top-user
  line-height 48px

.menu-top-user:after,
.menu-top-user:before
  display table
  line-height 0

.menu-wrap
  bottom 0
  left 0
  overflow-x hidden
  overflow-y auto
  position absolute
  right 0
  top 0

@media only screen and (min-width: 480px)
  .menu-wrap
    padding-right 32px
    right -32px

.menu-content .nav li > a:focus,
.menu-content .nav li > a:hover
  background-color lighten(grey,50%) !important

.menu-content .nav li.active > a
  color grey

for color_i in $palette_all
  .menu-content .nav .{color_i[0]} li > a:focus,
  .menu-content .nav .{color_i[0]} li > a:hover
    background-color lighten(color_i[1],50%) !important
  .menu-content .nav .{color_i[0]} li.active > a
    color color_i[1]

for color_i in $palette_all
  .navbar.bg-{color_i[0]} .nav > li > a .navbar-close i
    color white
    background-color color_i[1]
  .navbar.bg-{color_i[0]} .avatar i
    color color_i[1]

.content
  padding-top 55px
.content .page-header,
.content .content-heading
  margin 0 0 5px
.content .page-header .container,
.content .page-header .container-full,
.content .page-header .container-fluid
.content .content-heading .container,
.content .content-heading .container-full,
.content .content-heading .container-fluid
  margin-top 0 !important
.content .page-header h1,
.content .heading
  padding-top 48px
  padding-bottom 24px
  padding-left 10px

.fbtn {
  background-color: #e0e0e0;
  border-radius: 50%;
  box-shadow: 0 3px 10px rgba(0, 0, 0, 0.5);
  color: #212121;
  display: block;
  font-size: 24px;
  line-height: 45px;
  transition-delay: 0s;
  transition-duration: 0.3s;
  transition-property: box-shadow, opacity, transform;
  transition-timing-function: cubic-bezier(0.4, 0, 0.2, 1);
  width: 48px;
}
.card, .fbtn-text {
  border-radius: 2px;
}
a.fbtn:focus, a.fbtn:hover {
  box-shadow: 0 3px 10px rgba(0, 0, 0, 0.5), 0 3px 15px rgba(0, 0, 0, 0.5);
  color: #212121;
}
.fbtn-lg {
  height: 60px;
  line-height: 55px;
  text-align: center;
  width: 60px;
  margin-right: -6px;
}
.fbtn-lg .icon {
  padding-left: 6px;
}
.fbtn-lg .fbtn-sub {
  top: 18px;
}
.fbtn-container,
.scroll-to-top {
  position: fixed;
  z-index: 21;
}
.fbtn-container {
  bottom: 24px;
  right: 25px;
  transition: margin-bottom 0.3s cubic-bezier(0.4, 0, 0.2, 1) 0s, right 0.3s cubic-bezier(0.4, 0, 0.2, 1) 0s;
}
.scroll-to-top {
  bottom: 0;
  right: 19px;
  transition: opacity 0.3s cubic-bezier(0.4, 0, 0.2, 1) 0s, right 0.3s cubic-bezier(0.4, 0, 0.2, 1) 0s;
}
.scroll-to-top {
  opacity 0
  filter unquote("alpha(opacity=0)")
}
.scroll-to-top a {
  outline none !important
  color #ffffff
  padding 20px 20px 8px
  border 1px solid #cccccc
  background-color lighten(black,50%)
}
.scroll-to-top a:focus,
.scroll-to-top a:hover {
  color #ffffff
  border 1px solid #bbbbbb
  background-color lighten(black,20%)
  box-shadow 0 3px 10px rgba(0, 0, 0, 0.5), 0 3px 15px rgba(0, 0, 0, 0.5)
}
.page-offset .fbtn-container {
  margin-bottom 45px
}
.page-offset .scroll-to-top {
  bottom: -10px;
  height: 65px;
  opacity 1
  filter unquote("alpha(opacity=100)")
}
.fbtn-dropdown, .fbtn-sub, .fbtn-text {
  position: absolute;
}
.fbtn-dropdown {
  bottom: 100%;
  max-height: 0;
  padding-left: 16px;
  padding-right: 16px;
  right: -16px;
  transition: max-height 0s ease 0.5s;
}
.fbtn-inner.open .fbtn-dropdown {
  max-height: 99999px;
  overflow: visible;
  transition: max-height 0s ease 0s;
}
.fbtn-ori, .fbtn-rotate, .fbtn-sub {
}
.fbtn-dropdown .fbtn {
  margin: 12px auto;
  text-align: center;
  opacity: 0;
  filter unquote("alpha(opacity=0)")
  transform: scale(0.5);
}
.fbtn-dropdown .fbtn:nth-last-child(1) {
  transition-delay: 300ms;
}
.fbtn-dropdown .fbtn:nth-last-child(2) {
  transition-delay: 250ms;
}
.fbtn-dropdown .fbtn:nth-last-child(3) {
  transition-delay: 200ms;
}
.fbtn-dropdown .fbtn:nth-last-child(4) {
  transition-delay: 150ms;
}
.fbtn-dropdown .fbtn:nth-last-child(5) {
  transition-delay: 100ms;
}
.fbtn-dropdown .fbtn:nth-last-child(6) {
  transition-delay: 50ms;
}
.fbtn-inner.open .fbtn-dropdown .fbtn {
  opacity: 1;
  filter unquote("alpha(opacity=100)")
  transform: scale(1);
  transition-delay: 300ms;
}
.fbtn-inner.open .fbtn-ori, .fbtn-inner.open .fbtn-rotate {
  transform: rotate(225deg);
}
.fbtn-inner.open .fbtn-dropdown .fbtn:nth-last-child(1) {
  transition-delay: 0s;
}
.fbtn-inner.open .fbtn-dropdown .fbtn:nth-last-child(2) {
  transition-delay: 50ms;
}
.fbtn-inner.open .fbtn-dropdown .fbtn:nth-last-child(3) {
  transition-delay: 100ms;
}
.fbtn-inner.open .fbtn-dropdown .fbtn:nth-last-child(4) {
  transition-delay: 150ms;
}
.fbtn-inner.open .fbtn-dropdown .fbtn:nth-last-child(5) {
  transition-delay: 200ms;
}
.fbtn-inner.open .fbtn-dropdown .fbtn:nth-last-child(6) {
  transition-delay: 250ms;
}
.fbtn-ori, .fbtn-sub {
  transition-property: opacity, transform;
}
.fbtn-ori, .fbtn-rotate, .fbtn-sub {
  transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1) 0s;
}
.fbtn-inner.open .fbtn-ori {
  opacity: 0;
  filter unquote("alpha(opacity=0)")
}
.fbtn-rotate {
  transition-property: opacity, transform;
}
.fbtn-sub {
  left: 0;
  opacity: 0;
  filter unquote("alpha(opacity=0)")
  top: 12px;
  transform: rotate(-225deg);
  width: 100%;
}
.fbtn-inner.open .fbtn-sub {
  opacity: 1;
  filter unquote("alpha(opacity=100)")
  transform: rotate(0deg);
}
.fbtn-text {
  background-color: rgba(0, 0, 0, 0.8);
  font-size: 12px;
  height: 0;
  margin-right: 16px;
  margin-top: -12px;
  opacity: 0;
  filter unquote("alpha(opacity=0)")
  padding-left: 8px;
  padding-right: 8px;
  right: 100%;
  top: 50%;
  transition: opacity 0.3s cubic-bezier(0.4, 0, 0.2, 1) 0s;
  white-space: nowrap;
  width: 0;
}

for color_i in $palette_all
  .fbtn-{color_i[0]}
    background-color color_i[1]
    color color_i[2] !important
